{
  "apiVersion": "v1",
  "kind": "ConfigMap",
  "metadata": {
    "labels": {
      "app": "nginx"
    },
    "name": "nginx-config",
    "namespace": "new-namespace"
  },
  "data": {
    "nginx.conf": "user nginx",
    "worker_processes":  1,
    "events": {
      "worker_connections":  10240
    },
    "http": {
      "server": {
        "listen":80,
        "server_name": "localhost",
        "location": {
          "root": "/usr/share/nginx/html;",
          "index":  "index.html index.htm"
        }
      }
    }
  }
}